<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo 
	http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
	<!-- 1.设置应用名称 -->
	<dubbo:application name="jt-user"/>
	<!-- 2.配置zookeeper地址 -->
	<dubbo:registry address="zookeeper://192.168.121.135:2181"></dubbo:registry>
	<!-- 3.配置服务的端口号20880 -->
	<dubbo:protocol name="dubbo" port="20860"></dubbo:protocol>
	<!-- 4.注册服务实现类对象 -->
	<bean class="com.jt.sso.service.UserServiceImpl" id="userService"></bean>
	<!-- 5.配置接口名 -->
	<dubbo:service timeout="90000" interface="com.jt.dubbo.service.DubboUserService" ref="userService"/>
</beans>